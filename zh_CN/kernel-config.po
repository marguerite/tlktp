msgid "OneNAND Flash device via platform device driver"
msgstr ""

# MTD_ONENAND_GENERIC:00000
#: drivers/mtd/onenand/Kconfig:21
msgid "Support for OneNAND flash via platform device driver.\n"
msgstr ""

