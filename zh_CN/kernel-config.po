"ioctl(fd, _IO(ETRAXGPIO_IOCTYPE, IO_SETBITS), bits_to_set);\n"
"ioctl(fd, _IO(ETRAXGPIO_IOCTYPE, IO_CLRBITS), bits_to_clear);\n"
"val = ioctl(fd, _IO(ETRAXGPIO_IOCTYPE, IO_READBITS), NULL);\n"
